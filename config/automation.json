{
    "time_checks": {
      "cleanup": {
        "handler": "src.automation.routines.cleanup.CleanupRoutine",
        "interval": 3600
      },
      "donate": {
        "handler": "src.automation.routines.allianceDonate.AllianceDonateRoutine",
        "interval": 22000
      },
