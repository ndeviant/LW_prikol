        "handler": "src.automation.routines.claimSecretTasks.ClaimSecretTasks",
        "interval": 30
      },
      "assist_secret_tasks": {
        "handler": "src.automation.routines.assistSecretTasks.AssistSecretTasks",
        "options": {
          "min_star": 5,
          "claim_types": ["star", "hero"]
        },
        "interval": 45
      },
      "rally": {
        "handler": "src.automation.routines.rally.RallyRoutine",
        "interval": 5,
        "options": {
          "rally_types": ["golden"]
        }
      },
      "dig": {
        "handler": "src.automation.routines.checkForDigs.CheckForDigsRoutine",
        "interval": 10,
        "options": {
          "auto_claim": true,
          "wait_for_dig": 540,
          "wait_for_dig_interval": 0.248,
          "chat_search_swipes": 3
        }
      },
      "secretary": {
        "handler": "src.automation.routines.secretary.SecretaryRoutine",
        "interval": null
      },
      "mapExchange": {
        "handler": "src.automation.routines.mapExchange.MapExchangeRoutine",
        "interval": null
      },
      "gifts": {
        "handler": "src.automation.routines.allianceGifts.AllianceGiftsRoutine",
        "interval": 5000
      },
      "collect_resources": {
        "handler": "src.automation.routines.collectResources.CollectResourcesRoutine",
        "interval": 180
      }
    },
